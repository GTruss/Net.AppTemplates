{
  "Serilog": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Properties": {
      "ApplicationName": ".Net Web App"
    },
    "ColumnOptions": {
      "addStandardColumns": [ "TimeStamp", "Level", "Message", "Exception", "LogEvent" ],
      "removeStandardColumns": [ "MessageTemplate", "Properties" ],
      "additionalColumns": [
        {
          "ColumnName": "EventType",
          "PropertyName": "EventType",
          "DataType": "varchar",
          "DataLength": 10
        }
      ],
      "timeStamp": {
        "columnName": "Timestamp",
        "convertToUtc": false
      }
    },
    "SchemaName": "dbo",
    "TableName": "Log"
  }
}
